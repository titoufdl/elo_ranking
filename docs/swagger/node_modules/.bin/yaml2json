#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/iut45/Etudiants/o22203653/WinHome/NODEJS/elo_ranking/node_modules/.pnpm/yamljs@0.3.0/node_modules/yamljs/bin/node_modules:/home/iut45/Etudiants/o22203653/WinHome/NODEJS/elo_ranking/node_modules/.pnpm/yamljs@0.3.0/node_modules/yamljs/node_modules:/home/iut45/Etudiants/o22203653/WinHome/NODEJS/elo_ranking/node_modules/.pnpm/yamljs@0.3.0/node_modules:/home/iut45/Etudiants/o22203653/WinHome/NODEJS/elo_ranking/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/iut45/Etudiants/o22203653/WinHome/NODEJS/elo_ranking/node_modules/.pnpm/yamljs@0.3.0/node_modules/yamljs/bin/node_modules:/home/iut45/Etudiants/o22203653/WinHome/NODEJS/elo_ranking/node_modules/.pnpm/yamljs@0.3.0/node_modules/yamljs/node_modules:/home/iut45/Etudiants/o22203653/WinHome/NODEJS/elo_ranking/node_modules/.pnpm/yamljs@0.3.0/node_modules:/home/iut45/Etudiants/o22203653/WinHome/NODEJS/elo_ranking/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../yamljs/bin/yaml2json" "$@"
else
  exec node  "$basedir/../yamljs/bin/yaml2json" "$@"
fi
